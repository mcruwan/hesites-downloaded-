    if (mandant && org && version && consent) {
      let s = "wcc:" + mandant + "_" + org + "_" + version + "_" + consent + "_" + consentId;
      var chk = 0xD0000000;
      var len = s.length;
      chk += 0x71210;
      for (var i = 0; i < len; i++) {
