  _setCookie: function(name, value) {
    var exdate = new Date();
    exdate.setDate(exdate.getDate() + 365);

    var cookie = [
      name + '=' + value,
      'expires=' + exdate.toUTCString(),
      'path=' + '/'
    ];

    if (location.protocol === 'https:') {
      cookie.push('secure');
      cookie.push('SameSite=none');
    }

    document.cookie = cookie.join(';');

  },

