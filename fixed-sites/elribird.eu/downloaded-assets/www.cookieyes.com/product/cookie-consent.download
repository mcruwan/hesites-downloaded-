    <script>
        jQuery(document).ready(function($) {
            var timerEndTime = new Date('2026-01-31 23:59:59');
            var timerInterval;

            function updateTimer() {
                var now = new Date();
                var diff = timerEndTime - now;
                if (diff > 0) {
                    showtimervaluesprivacyweek(diff);
                } else {
                    clearInterval(timerInterval);
                    $(".timer-container").hide();
                }
            }

            function showtimervaluesprivacyweek(diff) {
                var days = Math.floor(diff / (1000 * 60 * 60 * 24));
                var hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                var minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
                var seconds = Math.floor((diff % (1000 * 60)) / 1000);

                $(".privacy-week-banner #days").text(days < 10 ? "0" + days : days);
                $(".privacy-week-banner #hours").text(hours < 10 ? "0" + hours : hours);
                $(".privacy-week-banner #minutes").text(minutes < 10 ? "0" + minutes : minutes);
                $(".privacy-week-banner #seconds").text(seconds < 10 ? "0" + seconds : seconds);
            }

            // Update timer immediately and then every second
            updateTimer();
            timerInterval = setInterval(updateTimer, 1000);
        });
    </script>
