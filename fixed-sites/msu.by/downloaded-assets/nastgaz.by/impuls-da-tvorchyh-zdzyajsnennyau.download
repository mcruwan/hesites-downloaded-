<script>
    function addUl() {
        let ul = document.querySelectorAll('.header__container-bottom #primary-menu .menu-item .sub-menu > li:not(.no-more) > ul') // sub ul

        let valIf
        let countShowLi
        
        if (document.documentElement.clientWidth >= 1300) {
            valIf = 2
            countShowLi = 3
        }
        
        if (document.documentElement.clientWidth < 1300 && document.documentElement.clientWidth >= 1100) {
            valIf = 1
            countShowLi = 2
        }
        
        if (document.documentElement.clientWidth < 1100) {
            valIf = -1
            countShowLi = 0
        }

        ul.forEach(elem => {
            if (elem.childElementCount > countShowLi) {
                let openUl = document.createElement('a')
                openUl.className = 'menu-open'
                openUl.innerHTML = '<img src="https://ng-press.by/wp-content/uploads/2021/11/plus.svg"> <span>Еще</span>'
                openUl.addEventListener('click', (e) => {
                    for (let j = 0; j < e.path[2].childElementCount; j++) {
                        e.path[2].children[j].style.display = 'block'
                    }
                    e.currentTarget.remove()
                })
                elem.append(openUl)
                for (let i = 0; i < elem.childElementCount; i++) {
                    if (i > valIf && elem.children[i].className != 'menu-open') {
                        elem.children[i].style.display = 'none'
                    }
                }
            }
        })
    }

    addUl()

    function mobileMenuSerch() {
        if (document.documentElement.clientWidth < 768) {
            var contentPalnel = document.querySelector('.header__content')
            var openerMenu = document.querySelector('.burger-menu')

            openerMenu.addEventListener('click', () => {
                openerMenu.classList.toggle('open')
                contentPalnel.classList.toggle('open')

                serchOpener.classList.remove('open')
                serchContent.classList.remove('open')
                mobileHeaderContainer.classList.remove('opener-for-serch')
            })
        }

        var serchContent = document.querySelector('.header__serch')
        var serchOpener = document.querySelector('.mobile-serch')
        var mobileHeaderContainer = document.querySelector('.header_mobile')
        serchOpener.addEventListener('click', () => {
            serchOpener.classList.toggle('open')
            serchContent.classList.toggle('open')
            mobileHeaderContainer.classList.toggle('opener-for-serch')

            openerMenu.classList.remove('open')
            contentPalnel.classList.remove('open')
        })
    }

    mobileMenuSerch()

    function desctopSerchControl() {
        if (document.documentElement.clientWidth > 768 && document.documentElement.clientWidth < 1401) {
            var serchContent = document.querySelector('.header__serch')
            var serchOpes = document.querySelector('.header__serch_open')
            var serchClose = document.querySelector('.header__serch_close')
            serchOpes.addEventListener('click', () => {
                serchOpes.classList.toggle('open')
                serchContent.classList.toggle('open')
                serchClose.classList.toggle('open')
            })
            serchClose.addEventListener('click', () => {
                serchOpes.classList.toggle('open')
                serchContent.classList.toggle('open')
                serchClose.classList.toggle('open')
            })
        }
    }

    desctopSerchControl()
