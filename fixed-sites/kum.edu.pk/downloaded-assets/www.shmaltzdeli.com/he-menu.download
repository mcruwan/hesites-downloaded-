<script src="ajax/libs/js-cookie/2.1.4/js.cookie.min.js"></script>
<script>
	$(function () {
        var lang = Cookies.get("setLanguage");
		/* SETUP DEFAULT LANGUAGE */
		if (lang == null) {
            Cookies.set("setLanguage", "en");
            lang = "en";
		}
		/* SETUP MULTI-LANGUAGE */
		var itemParent = "nav [class*='collection'],nav [class*='folder'],nav [class*='index']";
		$("nav a:link").each(function () {
			var langType = $(this).attr("href").split("-")[0].replace(/[^a-z0-9\s]/gi, "").replace(/[_\s]/g, "-");
			var multiLanguageClass = "multilanguage lang-" + langType + "";
			if (langType !== undefined && langType.length <= 2) {
				$(this).addClass(multiLanguageClass).parents(itemParent).addClass(multiLanguageClass);
			}
        });
        /* REMOVE HOME PAGE SECTIONS OF WRONG LANGUAGE */
        if(/^(\/|\/index)$/i.test(window.location.pathname)) {
            var parent = $("article#sections");
            var homeSections = parent.children("section.page-section");
            var childrenCount = (homeSections.length / 2);
            if (lang === "he") 
                homeSections.slice(0, childrenCount).remove();
            else
              // homeSections.slice(0, childrenCount).remove();
              homeSections.slice(childrenCount, homeSections.length).remove();
		}
		/* REMOVE FOOTER SECTION OF WRONG LANGUAGE */
		var footerparent = $("footer#footer-sections");
		var footerSections = footerparent.children("section.page-section");
		if (lang === "he") footerSections[0].remove();
		else footerSections[1].remove();
		/* CONDITINAL STATEMENTS FOR EACH LANGUAGE */
		if (lang == "en") {
			$("body").addClass("lang-en");
			//$('.index-section[id^="he"]').remove();
		}
		if (lang == "he") {
			$("body").addClass("lang-he");
			//$('.index-section[id^="en"]').remove();
			$("nav a:link[href*=tabitorder]").each(function () {
                $(this).text("הזמנה אונליין");
            });
			$("nav a:link[href*=hatchbeer]").each(function () {
                    $(this).text("'האטצ");
            });
            $("nav a:link[href*=valuecard]").each(function () {
                $(this).text("גיפט קארד");
            });
            $('nav a:link[href*=photos]').each(function() {
                $(this).text("תמונות")
            });
            $('nav a:link[href*=index]').each(function() {
                $(this).text("בית")
            });
            $('nav a:link[href*=shmaltzcatering]').each(function() {
                $(this).text("קייטרינג שמאלץ")
            });
		}
		/* ADD EXCLUSION NAV ITEMS */
		$(".sqs-svg-icon--list a,.SocialLinks-link").addClass("exclude");
		/* REMOVE OTHER LANGUAGES AND KEEP EXCLUDED ITEMS */
		$('.multilanguage:not(".lang-' + lang + ',.exclude")').remove();
		/* 1. SETUP LANGUAGE SWITCHER - TEXT */
		$("body").prepend(
			'<div class="language"><a href="https://www.shmaltzdeli.com/he-menu" class="lang-en">English</a> | <a href="https://www.shmaltzdeli.com/he-menu" class="lang-he">עברית</a></div>'
		);
		/* SET COOKIE LANGUAGE */
		$(".language a.lang-en").click(function () {
			Cookies.set("setLanguage", "en");
		});
		$(".language a.lang-he").click(function () {
			Cookies.set("setLanguage", "he");
		});
	});
</script>
