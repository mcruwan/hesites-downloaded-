<script>
	jQuery("#submit_email").click(function(){
			var email = jQuery("#signup_email").val();
			if(email == ''){
				alert('Please Enter a Email');
			}
		else{
			var link = 'https://app.geta.ai/signup/email=';
			var new_link = link.concat(email);
		  window.location.href = new_link;
		}
			
	});

</script>
<script>
jQuery('.pricing_tab_inner').click(function(){
			var plan = jQuery(this).attr('value');
		 if(plan === 'months'){
			 jQuery('.pricing_lists .col').hide();
			 jQuery('.months').show();
		 }
		else{
		jQuery('.pricing_lists .col').hide();
	}
});
</script>




