}

 .feature-card-wrapper {
    overflow: hidden;
    border-radius: 10px;
}

.feature-card {
    padding-top: 3rem;
    padding-bottom: 3rem;
    position: relative;
	border: 3px solid transparent;
  background: #ffff;
  background-clip: padding-box;
/* 	height: 45rem;
	min-height: 45rem; */
  
  border-radius: 10px;
    
    
 
  /*overflow: hidden;*/
  
  align-items: center;
  /*position: relative;*/
  box-shadow: 0 14px 26px rgba(0, 0, 0, 0.04);
  transition: all 0.3s ease-out;
  text-decoration: none;
 
}



.feature-card:after {
  content: '';
  position: absolute;
  top: -5px;
  bottom: -5px;
  left: -5px;
  right: -5px;
  background: linear-gradient(to bottom right, #D802FA, #0167FF);
  z-index: -1;
  border-radius: 10px;
}



.feature-card:hover svg path {
  fill: #000;
}

.feature-card:hover {
    
  transform: translateY(-5px) scale(1.005) translateZ(0);
  box-shadow: 0 24px 36px rgba(0, 0, 0, 0.11),
    0 24px 46px var(--box-shadow-color);
}

.feature-card:hover .feature-overlay {
  transform: scale(8) translateZ(0);
  
}

.feature-card:hover .feature-circle {
  border-color: var(--bg-color-light);
  background: var(--bg-color);
}

.feature-card:hover .feature-circle:after {
  background: #fff;
}

.feature-card:hover p.feature-header {
  color: var(--text-color-hover);
}

.feature-card:hover p.feature-description {
  color: var(--text-color-hover);
}

.feature-card:hover p {
  color: var(--text-color-hover);
}

.feature-card p.feature-header {
  font-size: 22px;
  color: #4c5656;
  text-align: center;
  position: relative;
  font-weight: 600;
  margin: 0;
  z-index: 1000;
  padding: 0 1.9rem;
  margin: 2rem 0rem 1rem 0;
  transition: color 0.3s ease-out;
  font-family: inter;
	letter-spacing:0px;
}

.feature-card p.feature-description {
/* 	letter-spacing: 0px; */
   font-size: 18px;
   padding: 0 3.2rem;
  color: #4c5656;
  text-align: center;
  position: relative;
  z-index: 1000;
  transition: color 0.3s ease-out;
  font-family: inter;
	line-height: 1.5em;
	letter-spacing: 0px
}

.feature-circle {
    /*border: 2px solid --bg-color-light;*/
  width: 118px;
  height: 118px;
  border-radius: 50%;
  background: #fff;
  
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  z-index: 1;
  left: 50%;
  transform: translateX(-50%);
  transition: all 0.3s ease-out;
}

.feature-circle:after {
  content: "";
  width: 100px;
  height: 100px;
  display: block;
  position: absolute;
  background-color: transparent;
    background-image: linear-gradient(130deg, #960BF2 32%, #1520A3 100%);
  border-radius: 50%;
  /*top: 7px;*/
  /*left: 7px;*/
  transition: opacity 0.3s ease-out;
}

.feature-circle svg {
    width: 5rem !important;
  z-index: 10000;
  transform: translateZ(0);
}

.feature-overlay {
  width: 118px;
  position: absolute;
  height: 118px;
  border-radius: 50%;
  background-color: transparent;
    background-image: linear-gradient(130deg, #960BF2 32%, #1520A3 100%);
  top: 3rem;
  left: 50%;
  transform: translateX(-50%);
  z-index: 0;
  transition: transform 0.3s ease-out;
}
