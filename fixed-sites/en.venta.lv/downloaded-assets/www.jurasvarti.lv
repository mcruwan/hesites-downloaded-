			if (window.innerWidth >= 1000 ){

					calcright();
			}

			if ($('.paseldc0').length != 0){
				if (scroll >= $('.paseldc0').offset().top && scroll < $('.zinas-loadmorebc').offset().top){
					if (lastscroll < scroll){
						if ($('.paseldc'+ pasfcnext).length != 0){
							if (scroll >= $('.paseldc'+ pasfcnext).offset().top ){
								pasfc++;
								pasfcnext++;
								pasfcprev++;

							}
						}
					} else {
						if ($('.paseldc'+ pasfcprev).length != 0){
							if (scroll <= $('.paseldc'+ pasfcprev).offset().top ){
								pasfc--;
								pasfcnext--;
								pasfcprev--;
							}
						}
					}

					if ($('.paseldc'+ pasfc).length != 0){
						if (scroll <= $('.paseldc'+pasfc).offset().top ){
							$('#fixedpashtext').html($('.paseldc'+pasfcprev).text());
							$('.is-pasakums-day').removeClass('scrolin');
							var dat = $('.paseldc'+pasfcprev).attr('date');
