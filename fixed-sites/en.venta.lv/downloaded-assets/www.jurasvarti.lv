						}
						if (scroll >= $('.paseldc'+pasfc).offset().top ){
							$('#fixedpashtext').html($('.paseldc'+pasfc).text());
							$('.is-pasakums-day').removeClass('scrolin');
							var dat = $('.paseldc'+pasfc).attr('date');
							$('#'+dat).addClass('scrolin');
						}
					}
					$('#fixedpash').fadeIn(0);
					lastscroll = scroll;
				} else {
					$('#fixedpash').fadeOut(0);
				}
			}


		},delay);
	});

