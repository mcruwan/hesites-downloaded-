function calcright(){
	var fix = $('.telpunomrightinc');
	var fixp = fix.parent();
	var fixpt = fixp.offset().top;
	var fixpb = fixp.offset().top + fixp.height() - fix.height();
	if (scroll >= fixpt && scroll <= fixpb){
		//fix.css('top', scroll - fixpt + 'px');
		fix.addClass('fixed');
	}
	if (scroll < fixpt){
		fix.css('top', 0 + 'px');
		fix.removeClass('fixed');
	}
	if (scroll > fixpb && fix.height() < fixp.height()){
		fix.removeClass('fixed');
		fix.css('top', 'auto');
		fix.css('bottom', 0);
	}
}

