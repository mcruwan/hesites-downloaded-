<!-- Cookie consent -->
<div id="cookieConsent" style="position: fixed; bottom: 10px; right: 10px; width: 300px; height: 100px; padding: 20px; background-color: #f1f1f1; border-radius: 5px; border: 1px solid #dbdbdb; font-size: 16px; display: none;">
    Šajā vietnē izmanto sīkdatnes, lai nodrošinatu Jūsu pieprasīto pakalpojumu darbību.
    <a href="#" onclick="acceptCookies();" style="display: block; background-color: #63bb00; color: #ffffff; text-decoration: none; text-align: center; padding: 10px; border-radius: 5px;">Sapratu</a>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script>
    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function acceptCookies() {
        setCookie("cookieConsent", "1", 365);
        $("#cookieConsent").fadeOut();
    }

    $(document).ready(function () {
        var consent = getCookie("cookieConsent");
        if (consent === "") {
            $("#cookieConsent").fadeIn();
        }
    });
</script>
