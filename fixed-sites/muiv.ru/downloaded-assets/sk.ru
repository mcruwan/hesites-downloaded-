                      <script>
const elDelay = 500;

document.addEventListener('DOMContentLoaded', () => {
  const membersBlocks = document.querySelectorAll('.member');
  window.addEventListener('scroll', function(e) {
    if (membersBlocks && membersBlocks.length > 0) {
      let ind = membersBlocks.length;
      while(ind--) {
        const aniBlock = membersBlocks[ind];
        if (elementInViewport(aniBlock) && !aniBlock.classList.contains('animationStart')) {
          animateMembers(aniBlock);
        } 
      }
    }
  });
});

function animateMembers(block) {
  const aniEls = block.querySelectorAll('.animation__block');
  if (aniEls && aniEls.length > 0) {
    block.classList.add('animationStart');
    for (let ind = 0; ind < aniEls.length; ind++) {
      animateEl(aniEls[ind], ind);
    }
  }
}

function animateEl(el, ind) {
  setTimeout(() => {
    if (!el.classList.contains('hover')) {
      el.classList.add('reColor');
      setTimeout(() => {
        el.classList.add('move');
        setTimeout(() => {
          el.classList.remove('move');
          el.classList.remove('reColor');
        }, elDelay);
      }, 200);
    }
  }, elDelay * ind );
}

function elementInViewport(el) {
  let top = el.offsetTop;
  const height = el.offsetHeight;

  while(el.offsetParent) {
    el = el.offsetParent;
    top += el.offsetTop;
  }

  if (height > window.innerHeight) {
    return (
      top < window.pageYOffset &&
      top > window.pageYOffset - 600
    );
  } else {
    return (
      top >= window.pageYOffset &&
      (top + height) <= (window.pageYOffset + window.innerHeight)
    );
  }
}
</script>
                    
