  <button type="button" class="btn btn-lg btn-secondary w-100" data-bind="click: $parent.showSelected.bind($parent), disable: $parent.chosen.selectedAnswers().length === 0">
      <span data-bind="text: textProperties.showChoices"></span>
      <span class="badge badge-lg bg-white text-secondary ml-2" data-bind="text: $parent.chosen.selectedAnswers().length"></span>
  </button>
  <!-- /ko -->
</script>

<script type="text/html" id="filterablecheckbox-mobile--filter-modal">
  <div class="modal fade forms-drawer-modal" tabindex="-1" role="dialog" data-bind="bsModalVisible: displayFilter">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-body w-100">
            <!-- ko with: displayModel -->
                <div class="mb-4 w-100" data-bind="template: { name: 'filterable-search' }"></div>

                <!-- ko foreach: filterableProperties -->
                <div class="mb-4">
                    <div class="mb-2 w-100">
                        <strong class="h5 font-weight-bold" data-bind="text:label"></strong>
                    </div>
                    <div class="pl-2">
                    <!-- ko foreach: filteredFilterItems -->
                        <div class="form-check mb-2">
                            <input class="form-check-input" type="checkbox" data-bind="checked: selected, enable: canSelect, attr: {id: 'f'+$index()+'-'+label}" />
                            <label class="form-check-label" data-bind="attr: {for: 'f'+$index()+'-'+label}">
                            <span><span data-bind="text:label"></span> (<span data-bind="text: count"></span>)</span>
                            </label>
                        </div>
                    <!-- /ko -->
                    </div>
                    <!-- ko if: showCollapse -->
                    <div class="pl-2">
                        <div class="cursor-pointer btn btn-link p-0 text-primary" data-bind="click: toggleCollapse">
                          <!-- ko if: isCollapsed -->
                          <span class="mr-1 ion-chevron-down"></span>
                          <span data-bind="formsResource: 'filterablecheckbox-filterUncollapse'"></span>
                          <span data-bind="text: '(' + filterItems().length + ')'"></span>
                          <!-- /ko -->
                          <!-- ko ifnot: isCollapsed -->
                          <span class="mr-1 ion-chevron-up"></span>
                          <span data-bind="formsResource: 'filterablecheckbox-filterCollapse'"></span>
                          <!-- /ko -->
                        </div>
                    </div>
                    <!-- /ko -->
                </div>
                <!-- /ko -->

            <!-- /ko -->
        </div>
        <div class="modal-footer bg-white" style="position:sticky; bottom:0;">
          <button type="button" class="btn btn-primary btn-lg w-100" data-dismiss="modal">
            <span data-bind="formsResource: 'filterablecheckbox-showResults'"></span> (<span data-bind="text: nrOfAnswers"></span>)
          </button>
        </div>
      </div>
    </div>
    <button type="button" style="position:fixed; top:0; right:0; font-size:1.2em; color: #eee; padding-right:0;" class="btn ion-close-round" data-dismiss="modal" aria-label="Close"></button>
  </div>
</script>


<script type="text/html" id="filterablecheckbox-mobile--filter-active">
  <div class="d-flex align-items-center flex-wrap">
    <!-- ko foreach: selectedFilterItems -->
    <div class="lead mb-1 mr-2">
      <a class="badge badge-default shadow-sm deselect-button cursor-pointer" role="button" data-bind="click: deselect, ">
        <span data-bind="text: label"></span>
        &times;
      </a>
    </div>
    <!-- /ko -->
  </div>
</script>

<script type="text/html" id="filterablecheckbox-mobile--found-results">
    <h5 class="font-weight-bold">
        <span data-bind="text: $parent.nrOfAnswers"></span>
        <span data-bind="formsResource: 'filterablecheckbox-foundResults'"></span>
    </h5>
</script><script type="text/html" id="select-template">
  <div class="answers">
    <select class="answer select form-control" data-bind="attr: {name: name}, disable: readonly, valueAfterChildren: chosen.value, css: {'is-invalid': validation.display() && !validation.isValid()}">
      <option value=""></option>
      <!-- ko foreach: availableAnswers -->
      <option data-bind="value: identifier, text: label, attr: { selected: $parent.chosen.containsAnswer($data) }"></option>
      <!-- /ko -->
      <!-- Fix for truncated options on iOS-devices -->
      <optgroup label="" style="display:none"></optgroup> 
    </select>
  </div>
</script><script type="text/html" id="info-template">
  <div class="answers">
    <div class="answer info">
      <textarea class="form-control" rows="4" data-bind="attr: {name: $parent.name, placeholder: '', readonly: readonly, autocomplete: autocompletekey }, textInput: value"></textarea>
    </div>
  </div>
</script><script type="text/html" id="text-template">
  <div class="answers">
    <div class="answer text">
      <input class="form-control" type="text" 
      data-bind="
        attr: {
          name: name, 
          placeholder: '', 
          readonly: readonly, 
          autocomplete: autocompletekey,
          list: 'suggestedValues' + identifier
        },
        textInput: value,
        event: { blur: onBlurValidation },
        css: {'is-invalid': validation.display() && !validation.isValid()}
      " />

      <datalist data-bind="attr: { id: 'suggestedValues' + identifier }, foreach: suggestedAnswers">
        <option data-bind="attr: { value: value }">
      </datalist>
    </div>
  </div>
</script><script type="text/html" id="password-template">
  <div class="answers">
    <div class="answer text">
      <input class="form-control" data-bind="attr: {name:name, placeholder: '', readonly: readonly}, textInput: value, event: { blur: onBlurValidation }, css: {'is-invalid': validation.display() && !validation.isValid()}" type="password" autocomplete="off"/>
    </div>
  </div>
</script><script type="text/html" id="matrix-template">
  <div class="fluid-container">
    <div data-bind="with:matrix, css: validation.css">
      <div class="row d-none d-sm-flex">
          <div class="col-sm-4"></div>
          <div class="col-sm-8">
            <div class="row" data-bind="foreach: headers">
              <div class="d-flex justify-content-sm-center" data-bind="css:$parents[1].layoutcolumnsClass">
                <label class="small"><span data-bind="text:$data, css:{'text-muted': $parents[1].readonly}"></span></label>
              </div>
            </div>
          </div>
      </div>
      <div data-bind="foreach:subQuestions">
        <div class="row py-1">
          <div class="col-sm-4">
            <label><span data-bind="text:label, css: {'text-danger': validation.display() && !validation.isValid()}"></span></label>
          </div>
          <fieldset class="col-sm-8">
            <div class="answers radio row" data-bind="foreach: availableAnswers" >
              <div class="answer d-flex justify-content-sm-center" data-bind="css:$parent.layoutcolumnsClass">
                <div class="form-check">
                  <input class="form-check-input" type="radio" data-bind="attr: {name: $parent.name, id: 'q'+$parent.identifier+'a'+identifier}, disable: $parent.readonly, value: identifier, checked:$parent.chosen.value" />
                  <label class="form-check-label d-sm-none" data-bind="text: label, attr: {for: 'q'+$parent.identifier+'a'+identifier}"></label>
                </div>
              </div>
            </div>
          </fieldset>   
        </div>
      </div>
    </div>
  </div>
</script><script type="text/html" id="searchabledropdown-template">
  <div class="answers">
    <select class="answer select form-control" 
    data-bind="
    selectPicker: { 
      value: chosen.value, 
      customAction: true, 
      customSearch: searchString, 
      refresh: forceRefresh, 
      placeholder: 'Zoeken...', 
      clearText: 'Wis keuze',
      isReadonly: readonly,
      isInvalid: validation.display() && !validation.isValid()
    }, 
    attr: {name: name }, disable: readonly">
      <!-- ko with: selectedAnswer -->
      <option data-bind="text: label, value: identifier" selected></option>
      <!-- /ko -->      
      <!-- ko foreach: searchAnswers -->
      <option data-bind="text: label, value: identifier"></option>
      <!-- /ko -->
      <!-- ko if: displayFallbackAnswers -->
      <!-- ko foreach: fallbackAnswers -->
      <option data-bind="text: label, value: identifier"></option>
      <!-- /ko -->
      <!-- /ko -->
    </select>
  </div>
</script><script type="text/html" id="date-template">
  <div class="answer" data-target-input="nearest" data-bind="attr: { id: 'date_'+identifier}, datepicker: {}">
      <input type="text" class="cursor-pointer form-control" data-bind="attr:{
        'data-target':'#date_'+identifier,
        readonly: readonly, 
        autocomplete: autocompletekey
      }, textInput: value" data-toggle="datetimepicker"/>
  </div>
</script><script type="text/html" id="dateofbirth-template">
    <div class="answer text">
      <input class="form-control" type="text" 
      data-bind="
        attr: {
          name: name, 
          placeholder: help, 
          readonly: readonly, 
          autocomplete: autocompletekey,
          list: 'suggestedValues' + identifier
        },
        textInput: value,
        event: { blur: onBlurValidation },
        css: {'is-invalid': validation.display() && !validation.isValid()}
      " />
    </div>
</script><script type="text/html" id="email-template">
  <div class="answers">
    <div class="answer email">
      <input class="form-control" type="email" autocorrect="off"
      data-bind="attr: {name:name, placeholder: '', readonly: readonly, autocomplete: autocompletekey }, textInput: value, event: { blur: onBlurValidation }, css: {'is-invalid': validation.display() && !validation.isValid()}" />
    </div>
  </div>
</script><script type="text/html" id="phone-template">
  <div class="answers">
    <div class="answer phone">
      <input class="form-control" type="tel" autocorrect="off"
      data-bind="attr: {name:name, placeholder: '', readonly: readonly, autocomplete: autocompletekey }, textInput: value, event: { blur: onBlurValidation }, css: {'is-invalid': validation.display() && !validation.isValid()}" />
    </div>
  </div>
</script><script type="text/html" id="name-template">
  <div class="answers">
    <div class="answer text">
      <input class="form-control" type="text" autocorrect="off" autocapitalize="on" 
       data-bind="attr: {name:name, placeholder: '', readonly: readonly, autocomplete: autocompletekey }, textInput: value, event: { blur: onBlurValidation }, css: {'is-invalid': validation.display() && !validation.isValid()}" />
    </div>
  </div>
</script><script type="text/html" id="rating-template">
  <div class="answers radio row" data-bind="foreach: options" >
    <div class="answer" data-bind="css:$parent.layoutcolumnsClass">
      <div class="form-check">
        <input class="form-check-input" type="radio" data-bind="attr: { name: $parent.name, id: 'q'+$parent.identifier+$data }, disable: $parent.readonly, value: $data, checked:$parent.value"></input>
        <label class="form-check-label" data-bind="text: $data, attr: {for: 'q'+$parent.identifier+$data}"></label>
      </div>
    </div>
  </div>
</script><script type="text/html" id="error-template">
  <div class="small">
    <span class="text-danger" data-bind="html:message" style="line-height:30px;"></span>
    <br/>
    <a href="#" data-bind="if: action, click:retryAction"><span data-bind="text: $parent.resources['error-tryAgain']"></span></a>        
  </div>
</script><script type="text/html" id="loading-template">
  <div class="spinner-border spinner-border-sm text-muted my-3" role="status">
    <span class="sr-only">Loading...</span>
  </div>
</script><script type="text/html" id="validation-template">
  <div>
    <span class="d-inline invalid-tooltip" data-bind="html:message"></span>
  </div>
</script><script type="text/html" id="loading-overlay">
    <div class="spinner-border text-secondary mb-4" role="status">
        <span class="sr-only">Loading...</span>
    </div>
</script>
      </div>
    </div>
  </div>

  <div id="app-info" class="fixed-bottom text-black-50 d-none d-sm-block"><small><small>5.0.1</small></small>
  </div>
</body>

</html>