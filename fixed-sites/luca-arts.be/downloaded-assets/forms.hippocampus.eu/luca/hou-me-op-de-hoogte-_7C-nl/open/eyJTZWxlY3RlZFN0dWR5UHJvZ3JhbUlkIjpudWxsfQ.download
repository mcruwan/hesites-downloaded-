<script type="text/html" id="questiontemplate">
  <div class="smt-question form-group">
    <div data-bind="attr: {'id': '/Question/' + identifier}">
      <!-- ko if: header -->
      <div class="row pt-3 pb-2">
        <div class="col-sm-12" data-bind="css:{'inline': inline}"><span data-bind="html:header"></span></div>
      </div>
      <!-- /ko -->
      <div class="row">
        <div data-bind="css:{'inline': inline, 'col-sm-4': inline, 'col-sm-12': !inline}">
          <label class="col-form-label forms-label" data-bind="css: {'text-danger': validation.display() && !validation.isValid()}" >
              <span class="position-absolute text-danger ml-n2 ml-sm-n3" data-bind="visible:required">*</span>
              <div><span data-bind="html:label"></span></div>            
          </label>
          <!-- ko if: help -->
          <div class="mt-n2"><small><em data-bind="html:help"></em></small></div>
          <!-- /ko -->
        </div>
        <fieldset class="templateContainer" data-bind="template: { name: template }, css:{'inline': inline, 'col-sm-6': inline, 'col-sm-12': !inline}"></fieldset>
      </div>
      <!-- ko foreach: feedback -->
        <div class="row">
          <fieldset class="templateContainer" data-bind="template: { name: template },css:{'inline': $parent.inline, 'col-sm-6': $parent.inline, 'offset-sm-4': $parent.inline, 'col-sm-12': !$parent.inline}"></fieldset>
        </div>
      <!-- /ko -->
      <!-- ko if: footer -->
      <div class="row pt-2 pb-3">
        <div class="col-sm-12" data-bind="css:{'inline': inline}"><span data-bind="html:footer"></span></div>
      </div>
      <!-- /ko -->
    </div>
  </div>
</script><script type="text/html" id="navigation">
