<script type="text/html" id="answer-header">
  <div class="d-flex align-items-center">
    <div class="form-check flex-fill">
      <input type="checkbox" class="form-check-input mr-2" data-bind="disable: answer.disabled, checkedValue: answer.identifier, checked: chosen.value, stopBubble" />
      <label class="form-check-label">
