  <div data-bind="visible: pages().length > 1">
    <nav aria-label="Page navigation">
      <ul class="pagination pagination-sm">
        <li class="page-item cursor-pointer" data-bind="click: setPreviousPage, css: { disabled: currentPage() === 0 }">
          <span class="page-link" aria-label="Previous"><span aria-hidden="true">&laquo;</span></span>
        </li>
        <!-- ko foreach: new Array(pages().length) -->
