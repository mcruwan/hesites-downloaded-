        </li>
        <!-- /ko -->
        <li class="page-item cursor-pointer" data-bind="click: setNextPage, css: { disabled: currentPage() === pages().length - 1 }">
          <span class="page-link" aria-label="Next"><span aria-hidden="true">&raquo;</span></span>
        </li>
      </ul>
    </nav>
  </div>
