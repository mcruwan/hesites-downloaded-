                <template data-templatekey="summaryTemplate">

                    <h3 id="SummaryLabel">Summary</h3>
                    <div class="row">
                        <div class="col">
                            <p id="pgSummary">
                                You are about to donate R {{FormattedTotal}} via {{PayMethod.Description}}.<br />
                                <span data-if="{{Frequency.IsRecurring}}">This will repeat {{Frequency.Description}} on the {{Frequency.DayOfTheMonth}} day of the month.<br />
                                </span>
                                <span data-if="{{Donor.Email}}">Confirmation emails will be sent to {{Donor.Email}}</span>
                            </p>
                        </div>
                        <div class="col">
                            <div id="pgProjectBreakDown">
                                Funds will be allocated to the following projects.              
                                 <ul data-repeat-donation="{{Donations}}">
                                     <li>{{donation.Name}}: R {{donation.FormattedAmount}}</li>                                    
                                 </ul>
                            </div>
                        </div>
                    </div>
                    <div id="pnlTerms" class="d-none">
                        <h3>
                            T&C - Original</h3>
                        <p id="pgTerms">
                            Below you will find some information about the terms and conditions.
                        </p>
                    </div>
                </template>

                <template data-templatekey="resultApprovedTemplate">
                    <h3>
                        Thank you.</h3>
                    <p id="pgSuccessMsg">
                        Your transaction has been processed.
                    </p>
                </template>

                <template data-templatekey="resultFailTemplate">
                    <h3>
                        Oh Dear.</h3>
                    <p id="pgFailureMsg">
                        Something went wrong with processing your transaction.  Please try again, or contact support.
                    </p>
                </template>

                <template data-templatekey="messageTemplate">
