                        <div class="spinner-border ms-auto" role="status" aria-hidden="true"></div>
                        <strong>{{Message}}</strong>
                    </div>
                </template>

                <template data-templatekey="paymentOptionError">
                    <h3>Payment option problem</h3>
                    <p>
                        The {{Description}} payment method is not available at this time<br />
                        Please select an alternate method, try again later, or contact support
                    </p>
                </template>

