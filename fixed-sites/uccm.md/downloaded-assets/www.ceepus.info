            } else {
                pagenavbar.removeClass("sticky");
                pagenavbar.removeClass("header-shadow");
                pagenavbarref.removeClass("sub-menubar-phantom");
                header.addClass("header-shadow");
            }
        }
    }

    const reduceHeader = () => {
        if ($(window).scrollTop() > scrollOffset) {  
            header.addClass("reduced");
            logo.addClass("reduced");
            logoh.addClass("reduced");
            myceepus.addClass("reduced");
            if (pagenavbarref.length) {
                pagenavbar.addClass("reduced");
                calculatePagenavbarOffsetTop();
            }
            
        } else {
            header.removeClass("reduced");
            logo.removeClass("reduced");
            logoh.removeClass("reduced");
            myceepus.removeClass("reduced");
            if (pagenavbar.length) {
                pagenavbar.removeClass("reduced");                
                calculatePagenavbarOffsetTop();
            }
        }
    }    

    const calculatePagenavbarOffsetTop = () => {
        pagenavbarOffsetTop = pagenavbarref.offset().top - header.height() - pagenavbar.height();
    };

    $(() => {
        header = $("#header");
        logo = $("#logo");
        logoh = $("#logoh");
        myceepus = $("#myceepus");

        pagenavbarref = $("#pagenavbarref");
        pagenavbar = $("#pagenavbar");
        pagenavbarOffsetTop = 0;

        if (pagenavbar.length) {
            calculatePagenavbarOffsetTop();
        }      

        stickMenu();

        $(window).scroll(()=>{        
            reduceHeader();
            stickMenu();        
        });   
    });
     
</script><input type="hidden" name="AutoLogoutIntervalMin" id="AutoLogoutIntervalMin" value="30" /><input type="hidden" name="DisableBackButton" id="DisableBackButton" value="False" /><input type="hidden" name="HelpLayout" id="HelpLayout" value="0" /><input type="hidden" name="LoggedIn" id="LoggedIn" value="0" /><!-- Modal Offcanvas Help Page-->
