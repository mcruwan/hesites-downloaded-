var tmnu;
$('a.item_menu').hover(
	function(){
		$('.submenu').hide();
		objmnu=$(this).parent().children('.submenu');
		if($(objmnu).css('display')!='none'){
			clearTimeout(tmnu);
		}
		else{
			$('.sub_menu').hide();
			clearTimeout(tmnu);
		}
		$(objmnu).slideDown(200);
	},
	function(){
		tmnu=setTimeout(function(){
			$(objmnu).hide();
		},200);
	}
);
$('.submenu').hover(
function(){
	clearTimeout(tmnu);
	$(this).show();
},
function(){
	$('.submenu').hide();

