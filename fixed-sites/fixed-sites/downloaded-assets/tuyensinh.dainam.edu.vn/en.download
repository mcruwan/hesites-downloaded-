<!-- Thông báo/tin tức tuyển sinh -->
<section class="notice py-5 my-xl-3">
  <div class="container" id="admission-news-container" data-base-url="/en/news/">
    <div class="row notice-row gutter-3-5">

      <div class="col-xl-6 col-12 position-relative col-left-xl-690">
          <h2 class="fw-extra mb-2 mb-xl-3">Admission announcement</h2>
          
          <div id="notice-list"></div>
          
          <a class="position-absolute notice-see-all hover-f666 d-flex gap-2 fw-extra fs-5" 
                            href="/en/new-categories/admission-announcement"
                      >
              <div class="fw-bold">See all announcements</div>
              <img src="/img/icon/see-all-plus.svg" alt="Icon see all"/>
          </a>
      </div>

      <div class="col-xl-6 col-12 position-relative mt-5 mt-xl-0 col-right-xl-690">
        <h2 class="fw-extra mb-2 mb-xl-3">Admissions news</h2>
          
          <div id="admission-new-list"></div>
          
          <a class="position-absolute notice-see-all hover-f666 d-flex gap-2 fw-extra fs-5" 
                            href="/en/new-categories/admissions-news-1"
                      >
              <div class="fw-bold">See all news</div>
              <img src="/img/icon/see-all-plus.svg" alt="Icon see all"/>
          </a>
      </div>

    </div>
  </div>
</section>

<script>
  document.addEventListener("DOMContentLoaded", function () {
    const noticeList = document.getElementById("notice-list");
    const admissionNewList = document.getElementById("admission-new-list");
    let newsContainer = document.getElementById("admission-news-container");
    let baseUrl = newsContainer.getAttribute("data-base-url");

    let noticeCategoryId = 34529
    if (noticeCategoryId !== 0){
      fetchNews(noticeList, noticeCategoryId);
    }

    let admissionNewCategoryId = 34530
    if (admissionNewCategoryId !== 0){
      fetchNews(admissionNewList, admissionNewCategoryId);
    }

    function fetchNews(newsList, categoryId) {
        let url = `/api/news/all?category=${categoryId}&lan=en`;

        fetch(url)
            .then(response => response.json())
            .then(data => {
                renderNews(newsList, data);
            })
            .catch(error => console.error("Error fetching news:", error));
    }

    function formatDate(timestamp) {
        if (!timestamp) return "";
        const date = new Date(timestamp * 1000);
        return date.toLocaleDateString("vi-VN");
    }
    
    function isToday(timestamp) {
        if (!timestamp) return false;
        const date = new Date(timestamp * 1000);
        const today = new Date();
        return date.getDate() === today.getDate() &&
              date.getMonth() === today.getMonth() &&
              date.getFullYear() === today.getFullYear();
    }
    
    function createNewsItem(news) {
        const newsItem = document.createElement("a");
        newsItem.classList.add("notice-item");
        newsItem.href = baseUrl + news.slug;
    
        const formattedDate = formatDate(news.created_date);
        const highlightClass = isToday(news.created_date) ? "new" : "";
    
        const imgHtml = news.banner
            ? `<div class="notice-item-img-container">
                  <div class="zoom-105 rounded-12">
                      <img src="${news.banner}" width="100%" alt="New Banner" loading="lazy" />
                  </div>
              </div>`
            : "";
    
        newsItem.innerHTML = `
            ${imgHtml}
            <div class="notice-item-content-container">
                <div class="color-primary notice-item-date fs-6 fw-medium mb-lg-2">
                    ${formattedDate}
                </div>
                <div class="color-primary fw-extra notice-item-content ${highlightClass}">
                    ${news.title}
                </div>
            </div>
        `;
    
        return newsItem;
    }
    
    function renderNews(newsList, newsData) {
        newsList.innerHTML = ""; // Xóa danh sách cũ
    
        if (!newsData || newsData.length === 0) {
            newsList.innerHTML = "<p>Không có tin tức nào.</p>";
            return;
        }
    
        newsData.forEach(news => newsList.appendChild(createNewsItem(news)));
    }
  
});


</script>
                
