            {{/if}}
            <div class="stars stars-{{rate}}">
                <div class="star s-1"></div>
                <div class="star s-2"></div>
                <div class="star s-3"></div>
                <div class="star s-4"></div>
                <div class="star s-5"></div>
            </div>
