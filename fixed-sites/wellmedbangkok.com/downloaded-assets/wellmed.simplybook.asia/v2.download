            <div class="circle"></div> - {{_t 'available'}}
        </div>
        <div class="unavailable">
            <div class="circle"></div> - {{_t 'unavailable'}}
        </div>
    {{/ifCond}}


    {{#ifCond time_diff '!=' undefined}}
