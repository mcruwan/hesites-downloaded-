    {{/ifCond}}

    <div class="one-line {{#ifCond (getThemeOption 'hide_img_mode') '==' 1}}no-image{{/ifCond}}">
        <div class="content">
            {{#ifCond (lengthWithoutTags description) ">=" 2}}
