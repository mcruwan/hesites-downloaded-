						if((scrollTop <= mheaderFixedHeight + mheaderTop))
						{
							isMHeaderFixed = false;
							mfixed.removeClass('fixed');
						}
					}
				}
			}
			else
				mfixed.removeClass('fixed');
		})
	}
}

