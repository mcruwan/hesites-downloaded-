					}
					else if(scrollTop <= mheaderFixedHeight + mheaderTop){
						$('body.mfixed_Y.mfixed_view_scroll_top #mobileheader').removeClass('fixed');
						$('body.mfixed_Y.mfixed_view_scroll_top #mobileheadersimple').removeClass('fixed');
					}
