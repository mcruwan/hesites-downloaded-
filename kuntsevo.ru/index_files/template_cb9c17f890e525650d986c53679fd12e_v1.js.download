var funcDefined = function(func){
	try
	{
		if(typeof func == 'function')
			return true;
		else
			return typeof window[func] === "function";
	}
	catch (e)
	{
		return false;
	}
}

getRandomInt = function(min, max){
	return Math.floor(Math.random() * (max - min)) + min;
}

